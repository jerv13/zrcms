{
  "name": "zrcms/core",
  "description": "Reliv Content Management System 2",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "content",
    "manager",
    "zend",
    "expressive",
    "middleware",
    "module",
  ],
  "homepage": "http://www.reliv.com/",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "http://www.reliv.com/"
    },
    {
      "name": "Rod McNew",
      "email": "rmcnew@relivinc.com",
      "homepage": "http://www.reliv.com/"
    }
  ],
  "require": {
    "php": ">=7",
    "psr/container": "^1"
    "psr/http-message": "^1",
    "reliv/zf-config-factories": "^4",
    "zendframework/zend-expressive": "^1.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Zrcms\\Core\\": "core/",
      "Zrcms\\CoreDoctrine\\": "core-doctrine/"
    }
  },
  "suggest": {
    "dasprid/container-interop-doctrine": "^0.2.2",
    "rwoverdijk/assetmanager": "1.*"
  },
  "extra": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-installer-paths": {
      "bower-asset-library": "public/bower_components"
    }
  },
  "config": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-installer-paths": {
      "bower-asset-library": "public/bower_components"
    }
  }
}
